<Configuration status="WARN">
    <Appenders>
        <Console name="CONSOLE" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} %level - [%-8c{1}] %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="io.afloatdb" level="INFO">
            <AppenderRef ref="CONSOLE"/>
        </Logger>
        <Logger name="io.microraft" level="INFO">
            <AppenderRef ref="CONSOLE"/>
        </Logger>
    </Loggers>
</Configuration>
